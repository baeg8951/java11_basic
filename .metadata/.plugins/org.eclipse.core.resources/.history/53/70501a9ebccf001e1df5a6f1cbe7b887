package org.kh.java;

public class Variable6 {

	public static void main(String[] args) {
		float f1 = 3.14F;
		double f2 = 3.14;
		int kor=90, eng=80, mat=90;
		//float avg = (kor + eng + mat) / 3.0f; 둘다 정수면 정수가 나와서 하나는 실수로 변경 필요여서(float)을 =  이후 기입
		float avg = (float)(kor + eng + mat) / 3;
		System.out.printf("\nf1=%.3f", f1);
		System.out.printf("\nf1=%.2f", f2);
		System.out.printf("\n평균 : %.2f", avg);
		int a=10;
		float b=2.0f;
		float sum = (float)(a +b);
		System.out.println(a +b);
	}
}
